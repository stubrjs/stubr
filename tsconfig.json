{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "es5",
		"noImplicitAny": false,
		"sourceMap": true,
		"experimentalDecorators": true,
		"resolveJsonModule": true,
		"strict": true
	},
	"typeRoots": [
		"./node_modules/@types",
		"./src/lib/typings"
	],
	"exclude": [
		"node_modules"
	]
}
